	<parameter name="partner_id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select rp.name, ai.date_invoice, ai.number, ai.amount_total, ai.residual, ai.leyenda_pp, ru.id usuid, ru.name usuname, rc.symbol, ra.city, ra.street, ra.phone from account_invoice as ai
join res_partner_address as ra on ai.address_invoice_id = ra.id
join res_partner as rp on ai.partner_id = rp.id
join res_currency as rc on ai.currency_id = rc.id
join res_users as ru on ai.user_id = ru.id
where ai.type='out_invoice' and ai.state = 'open' and rp.id = $P{partner_id}
group by rp.name, ai.date_invoice, ai.number, ai.amount_total, ai.residual, ai.leyenda_pp, ru.id, ru.name , rc.symbol, ra.city, ra.street, ra.phone
order by ru.name,ra.city,ai.date_invoice]]>
